{
  "name": "spektrumzywienia",
  "private": true,
  "description": "basic Gatsby starter with Styled Components & linters",
  "version": "0.1.0",
  "author": "Krzysztof Hankiewicz @kjhank <kjhank@gmail.com>",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@fortawesome/fontawesome-pro": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.4",
    "@fortawesome/pro-light-svg-icons": "^5.15.4",
    "@fortawesome/pro-regular-svg-icons": "^5.15.4",
    "@fortawesome/pro-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "babel-plugin-styled-components": "^1.13.1",
    "dotenv": "^10.0.0",
    "gatsby": "^3.8.1",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-image": "^1.8.0",
    "gatsby-plugin-manifest": "^3.9.0",
    "gatsby-plugin-offline": "^4.9.0",
    "gatsby-plugin-react-helmet": "^4.9.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^3.9.0",
    "gatsby-plugin-styled-components": "^4.9.0",
    "gatsby-source-filesystem": "^3.9.0",
    "gatsby-source-rest-api": "^0.2.3",
    "gatsby-transformer-sharp": "^3.9.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "polished": "^4.1.3",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sanitize-html": "^2.5.0",
    "smoothscroll-polyfill": "^0.4.4",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.4",
    "lint-staged": "^10.5.3",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn eslint",
      "yarn stylelint \"app/**/*.js\""
    ],
    "*.jsx": [
      "yarn eslint"
    ]
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "lint": "eslint . && stylelint \"app/**/*.js\"",
    "nuke": "gatsby clean && rm -fr node_modules && yarn install",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "start": "yarn run develop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kjhank/gatsby-styled-components"
  },
  "bugs": {
    "url": "https://github.com/kjhank/gatsby-styled-components/issues"
  }
}
